node{

  stage ('scm'){
   // git clone https://github.com/spring-projects/spring-petclinic.git

	 git 'https://github.com/spring-projects/spring-petclinic.git'   
  }
	stage ('build package') {
	  // mvn package
	  sh 'sudo mvn package'
	}
	stage ('archiveing') {
	  // archive artifact
	  archiveArtifacts 'target/**/*'
 }
	
}
